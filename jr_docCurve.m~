function docMatrix = jr_docCurve(decValues)

cutOffVector = linspace(-2,2,11);

docMatrix = zeros( (size(decValues, 1)* 2 ) , size(cutOffVector, 2))

for i = 1 : size(cutOffVector, 2)
    
    
    
end

decValues(1,:);

boundary = zeros(1, size(decValues, 2));

decisions = zeros(1, size(decValues, 2));

for i = 1 : size(decValues, 2)
    if decValues(1, i) > boundary(i)
        decisions(i) = 1;
    end
end

% find proportion of true positives and proportion of false positives
testLabelVector = jr_svm_genTestLabelVector(5, 1);

numTruePositives = 0;
numFalsePositives = 0;
for i = 1 : size(decValues, 2)
    if testLabelVector(i) == 1
        numTruePositives = numTruePositives +  decisions(i);
    else
        numFalsePositives = numFalsePositives +  decisions(i);
    end
end

end




